@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@using Capstone.Web.Models
@model LandmarkModel
<div id="landmark-details">
    <div id="landmark-details-container">

        <img src="~/Content/imgs/Landmarks/@Model.image" />
        <div id="landmark-details-name">
            <p>@Model.name</p>
        </div>
        <div>
            <p>
                @Model.address
            </p>
            <p>
                @Model.description
            </p>
        </div>
    </div>
</div>
