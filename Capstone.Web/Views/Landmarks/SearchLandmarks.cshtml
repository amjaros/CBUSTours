@{
    ViewBag.Title = "Search Landmarks";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using Capstone.Web.Models
@model List<LandmarkModel>

<h2>@ViewBag.Title</h2>

@foreach(LandmarkModel LM in Model)
{ 
    <div>
       
        <a href="~/Landmarks/LandmarkDetails?landmarkID=@LM.landmark_id">@LM.name</a>
    </div>
}